{
    "name": "Python base environment",
    "id": "python",
    "version": "0.0.13",
    "description": "Installs Python runtimes with pyenv",
    "options": {
      "defaultPythonVersion": {
        "type": "string",
        "default": "3.11.4",
        "description": "The Python runtime which gets installed in the default user's home environment. If the value is system, no runtime gets installed and pyenv is configured to use the system Python version."
      }
    },
    "customizations": {
        "vscode": {
          "extensions": [
            "ms-python.python",
            "bungcip.better-toml",
            "ms-toolsai.jupyter"
          ]
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils"
    ]
}
